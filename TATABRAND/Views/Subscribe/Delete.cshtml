@model TataPk
<div class="container">
    <div class="row">
        <div class="col-md-12 border border-1">

            <h1 class="text-uppercase text-center">delete</h1>
        </div>
        <div class="row">

            <div class="col-md-8 border border-1">
                <form asp-action="Delete" asp-controller="Subscribe" method="post" enctype="multipart/form-data">
                    <input asp-for="@Model.Logo" hidden/>
                    <input asp-for="@Model.ID" hidden />
                    <div class="mb-3">
                        <label asp-for="@Model.Name" class="form-label"></label>
                        <input asp-for="@Model.Name" class="form-control" disabled/>
                       
                    </div>
                    <div class="mb-3">
                        <label asp-for="@Model.EstablishYear" class="form-label"></label>
                        <input asp-for="@Model.EstablishYear" class="form-control"disabled/>
                        
                    </div>
                    
                    <div class="mb-3">
                        <a asp-controller="Subscribe" asp-action="subscribe" class="btn btn-warning text-uppercase">back</a>
                        <button type="submit" class="btn btn-danger">delete</button>
                    </div>
                </form>
            </div>
            <div class="col-md-4">
                @if (Model.Logo != null)
                {
                    <img src="@Model.Logo" width="100%">

                }
            </div>
        </div>
    </div>
</div>
