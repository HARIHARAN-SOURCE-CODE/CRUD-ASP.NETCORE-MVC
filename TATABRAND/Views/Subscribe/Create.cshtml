@model TataPk
<div class="container">
    <div class="row">
        <div class="col-md-12 border border-1">
      
            <h1 class="text-uppercase text-center">create</h1>
        </div>
        <div class="row">
            <div class="col-md-8 border border-1">
                <form asp-action="Create" asp-controller="Subscribe" method="post" enctype="multipart/form-data">
                    <div class="mb-3">
                        <label asp-for="@Model.Name" class="form-label"></label>
                        <input asp-for="@Model.Name" class="form-control">
                        <span asp-validation-for="@Model.Name" class="text-danger"></span>
                    </div>
                    <div class="mb-3">
                        <label asp-for="@Model.EstablishYear" class="form-label"></label>
                        <input asp-for="@Model.EstablishYear" class="form-control">
                        <span asp-validation-for="@Model.EstablishYear" class="text-danger"></span>
                    </div>
                    <div class="mb-3">
                        <label asp-for="@Model.Logo" class="form-label"></label>
                        <input asp-for="@Model.Logo" type="file" class="form-control" />
                       
                    </div>
                    <div class="mb-3">
                        <a asp-controller="Subscribe" asp-action="subscribe" class="btn btn-danger text-uppercase">back</a>
                      <button type="submit" class="btn btn-success">Create</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <partial name="_ValidationScriptsPartial" />
}
